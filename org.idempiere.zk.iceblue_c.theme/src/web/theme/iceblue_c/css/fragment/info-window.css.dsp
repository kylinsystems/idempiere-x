.info-panel .z-window-overlapped, .info-panel .z-window-highlighted {
	background-color: #F4F4F4 !important;
}
.info-panel .z-window-highlighted .z-window-content {
	padding: 0px;
}
.info-panel.z-window .z-borderlayout > div > .z-north {
	max-height: 50%;
}
.info-panel.z-window .z-borderlayout > div > * {
	background: #c7e8ff;
}
.info-panel.z-window .z-borderlayout .z-tabpanel {
	background: #c7e8ff;
	border: none;
	padding-top: 0px;
    padding-bottom: 0px;
	padding-left: 0px;
	padding-right: 0px;
}
.info-panel.z-window .z-grid-body {
	background: #c7e8ff;
}

.info-panel .z-grid {
	border: none !important;
}

.info-panel .z-listbox {
	width: 100% !important;
	position: relative;
	margin: auto;
	border-bottom: 0px solid #cccccc;
	border-left: 0px solid #cccccc;
	border-right: 0px solid #cccccc;
}
.info-panel .z-listheader {
	border-bottom: 0px;
}
.info-panel .z-listheader > .z-listheader-content {
	white-space: pre-line;
	text-overflow: ellipsis;
}

.info-panel .z-south {
	margin-top: 0px;
}
.info-panel .z-south-header {
    background: #7ac8ff;
}

.info-panel .statusBar > div > .z-west,  .info-panel .statusBar > div > .z-east {
	border: none;
}
